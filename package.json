{
	"name": "matrix-engine-examples",
	"version": "1.2.1",
	"description": "How to use matrix engine from npm service. MatrixEngine is opengles2.0/3.0 ObjectOriented scene engine based on glmatrix engine lib. PWA Fully powered with static/dynamic cache , add to home page in same time fixed autoplay audio/video context construction.",
	"main": "app.js",
	"type": "module",
	"scripts": {
		"examples": "watchify App-Examples.js -p [esmify --noImplicitAny] -o builds/examples.me.js",
		"anatomy": "watchify projects/web-anatomy/App.js -p [esmify --noImplicitAny] -o builds/web-anatomy.js",
		"slot": "watchify projects/matrix-slot/App.js -p [esmify --noImplicitAny] -o builds/slot.js",
		"roulette": "watchify projects/matrix-roulette/App.js -p [esmify --noImplicitAny] -o builds/roulette.js",
		"hang3d": "watchify projects/hang3d/App.js -p [esmify --noImplicitAny] -o builds/hang3d.js",
		"build.roulette": "browserify projects/matrix-roulette/App.js -p esmify > builds/roulette.js",
		"build.hang3d": "browserify projects/hang3d/App.js -p esmify > builds/hang3d.js",
		"build.examples": "browserify App-Examples.js -p esmify > builds/examples.me.js",
		"build.app": "browserify App.js -p esmify > builds/app.js",
		"build.slot": "browserify projects/matrix-slot/App.js -p esmify > builds/slot.js",
		"build.web.anatomy": "browserify projects/web-anatomy/App.js -p esmify > builds/web-anatomy.js",
		"build.all": "npm run build.examples | npm run build.app | npm run build.slot | npm run build.web.anatomy",
		"info": "echo \"Matrix engine writen in ECMA6. \n  From begin it was opengles1.1. For switch 3d context [webgl/webgl2] [shader opengles 300/2.0/1.1] see readme docs - Enjoy!  \n  Nikola Lukic @zlatnaspirala maximumroulette.com 2024 \""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zlatnaspirala/matrix-engine.git"
	},
	"keywords": [
		"matrix-engine",
		"matrix-engine-examples",
		"raycast-triangle",
		"3d-font",
		"vertices-font",
		"plane-text",
		"UV-fonts",
		"pwa",
		"powerfull",
		"fast-render",
		"alternative-3d",
		"alternative",
		"webgl",
		"webgl2",
		"glmatrix",
		"glmatrix2",
		"opengles1.1",
		"opengles2",
		"opengles300",
		"popMatrix",
		"pushMatrix",
		"blender-glmatrix-export",
		"obj-loader",
		"obj-sequence",
		"bvh",
		"games",
		"3d-games",
		"web-anatomy",
		"3d-roulette",
		"3d-roulette-webgl",
		"3d-roulette-real-physics",
		"web-anatomy",
		"First-person-shooter",
		"open-source",
		"mobile",
		"android"
	],
	"author": "Nikola Lukic zlatnaspirala",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/zlatnaspirala/matrix-engine/issues"
	},
	"homepage": "https://github.com/zlatnaspirala/matrix-engine#readme",
	"dependencies": {
		"audio-commander": "^1.0.3",
		"fs": "0.0.1-security",
