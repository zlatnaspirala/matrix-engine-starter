{
  "name": "matrix-engine-examples",
  "version": "1.0.0",
  "description": "MatrixEngine is opengles2.0/3.0 ObjectOriented scene engine based on glmatrix engine lib. PWA Fully powered with static/dynamic cache , add to home page in same time fixed autoplay audio/video context construction.",
  "main": "app.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build.examples": "browserify App-Examples.js -p esmify > builds/examples.me.js",
    "build.app": "browserify App.js -p esmify > builds/app.js",
    "build.slot": "browserify projects/matrix-slot/App.js -p esmify > builds/slot.js",
    "build.all": "npm run build.examples | npm run build.app | npm run build.slot",
    "test": "echo \"Error: no test specified\" && exit 1",
    "info": "echo \"Matrix engine writen in ECMA6. \n  Enjoy!  \n  Nikola Lukic @zlatnaspirala  \""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zlatnaspirala/webgl2-glmatrix2-engine.git"
  },
  "keywords": [
    "raycast-triangle",
    "3d-font",
    "vertices-font",
    "plane-text",
    "UV-fonts",
    "pwa",
    "powerfull",
    "fast-render",
    "alternative-3d",
    "alternative",
    "webgl",
    "webgl2",
    "glmatrix",
    "glmatrix2",
    "opengles1.1",
    "opengles2",
    "opengles300",
    "popMatrix",
    "pushMatrix"
  ],
  "author": "Nikola Lukic Zlatnaspirala",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zlatnaspirala/webgl2-glmatrix2-engine/issues"
  },
  "homepage": "https://github.com/zlatnaspirala/webgl2-glmatrix2-engine#readme",
  "dependencies": {
    "matrix-engine": "^1.4.3",
    "matrix-engine-plugins": "0.0.7",
    "voice-commander": "0.0.4"
  },
  "devDependencies": {
    "esmify": "^2.1.1"
  }
}
